{
  "name": "speedyreact-kit",
  "packageManager": "yarn@4.2.2",
  "homepage": "https://santoshshinde2012.github.io/speedyreact-kit",
  "scripts": {
    "build": "node script/build.mjs",
    "build-analyze": "node script/build.mjs --analyze=true",
    "dev": "node script/serve.mjs",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "jest",
    "prepare": "husky || true",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\" \"script/**/*.{js,mjs,ts,tsx,json,css,scss}\"",
    "precommit": "yarn lint-fix && yarn format",
    "prepush": "yarn run lint"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.4",
    "esbuild-plugin-svg": "^0.1.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
